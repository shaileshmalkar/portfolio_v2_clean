{
  "version": 2,
  "buildCommand": "cd frontend && npm install && npm run build",
  "outputDirectory": "frontend/dist",
  "installCommand": "cd frontend && npm install",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/profile",
      "destination": "/api/profile.py"
    },
    {
      "source": "/api/skills",
      "destination": "/api/skills.py"
    },
    {
      "source": "/api/blog",
      "destination": "/api/blog.py"
    },
    {
      "source": "/api/resume",
      "destination": "/api/resume.py"
    },
    {
      "source": "/api/projects",
      "destination": "/api/projects.py"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
